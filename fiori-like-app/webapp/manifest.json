{
  "_version": "1.49.0",
  "sap.app": {
    "id": "fiorilikeapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.10.1",
      "toolsId": "b2529d7c-17b7-408e-a1fe-5fdd83bfb614"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.116.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ui.layout": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "fiorilikeapp.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "rootView": {
      "viewName": "fiorilikeapp.view.App",
      "type": "XML",
      "async": true,
      "id": "idAppContainer"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "path": "fiorilikeapp.view",
        "controlId": "idAppContainer",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "routeMain",
          "target": "targetMain"
        },
        {
          "pattern": "details",
          "name": "routeDetails",
          "target": "targetDetails"
        }
      ],
      "targets": {
        "targetMain": {
          "viewName": "View1",
          "viewId": "idView1",
          "controlAggregation": "masterPages"
        },
        "targetDetails": {
          "viewName": "Details",
          "viewId": "idDetails",
          "controlAggregation": "detailPages"
        }
      }
    }
  }
}
