<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="webApp.controller.View1"
	xmlns:f="sap.ui.layout.form"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Title">
				<content>
					<f:SimpleForm editable="true">
						<f:content>
							<Label text="Emp ID"></Label>
							<Input id="empId" value= "{/empId}"></Input>
							<Label text="Emp Name"></Label>
							<Input id="empName" value= "{/empName}"></Input>
							<Label text="Emp Salary"></Label>
							<Input id="empSalary" value= "{/salary}"></Input>
							<Label text="Emp Currency"></Label>
							<Input id="empCurrency" value= "{/currency}"></Input>
							<Button id="idBtn" text="Submit" press="onSubmit"></Button>
						</f:content>
					</f:SimpleForm>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>